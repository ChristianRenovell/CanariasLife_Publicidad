<% include partials/_header %>
<div class="container mt-5">  
    <div class="row text-center">
        <div class="col-lg-6">
            <h1 class="text-uppercase mb-4"><%= nameBanner %></h1>
            <h2 class="text-primary">Historial del Banner</h2>
            <a href="/publi_services/reportpdfBanner/<%= id %>/<%= nameBanner %>" class="btn btn-outline-danger mt-3 mb-3 btn-block">Generar PDF</a>
            <% if (dataBanner) { %>
 
            <% for(var i = 0; i < dataBanner.length; i++) { %>
            <table>
            <tr>
            <%
                    let horaB = dataBanner[i].date.getHours();
                    let minutoB = dataBanner[i].date.getMinutes();
                    let segundosB = dataBanner[i].date.getSeconds();
                    
                    if(horaB <= 9) {
                        horaB = "0"+ dataBanner[i].date.getHours();
                    } 
                    if(minutoB <= 9) {
                        minutoB = "0"+ dataBanner[i].date.getMinutes();
                    }
                    if(segundosB <= 9) {
                        segundosB = "0"+ dataBanner[i].date.getSeconds();
                    }
               
                %>
                <td><h4><%= dataBanner[i].date.getDate() %>/<%= dataBanner[i].date.getMonth()+1 %>/<%= dataBanner[i].date.getFullYear() %> <%= horaB %>:<%= minutoB %>:<%= segundosB %> Hora de las Islas Canarias .</h4></td>
            </tr>
            </table>
            <% } %>
            <% } %>
        </div>
        <div class="col-lg-6">
            <h1 class="text-uppercase mb-4"><%= nameVideo %></h1>
            <h2 class="text-primary">Historial del Vídeo</h2>
            <a href="/publi_services/reportpdfVideo/<%= id %>/<%= nameVideo %>" class="btn btn-outline-danger mt-3 mb-3 btn-block" id="testButton">Generar PDF</a>
            <% if (dataVideo) { %>
            <% for(var i = 0; i < dataVideo.length; i++) { %>
            <table>
            <tr>
                 <%
                let horaV = dataVideo[i].date.getHours();
                let minutoV = dataVideo[i].date.getMinutes();
                let segundosV = dataVideo[i].date.getSeconds();
                
                if(horaV <= 9) {
                    horaV = "0"+ dataVideo[i].date.getHours();
                } 
                if(minutoV <= 9) {
                    minutoV = "0"+ dataVideo[i].date.getMinutes();
                }
                if(segundosV <= 9) {
                    segundosV = "0"+ dataVideo[i].date.getSeconds();
                }
                %>
           
                <td><h4><%= dataVideo[i].date.getDate() %>-<%= dataVideo[i].date.getMonth()+1 %>-<%= dataVideo[i].date.getFullYear() %> <%= horaV %>:<%= minutoV %>:<%= segundosV %> Hora de las Islas Canarias.</h4></td>
            </tr>
            </table>
            <% } %>
            <% } %>
         
        </div>
    </div>
    <div class="text-center">
        <a class="btn btn-primary mt-5" style="width: 20%;" href="javascript:history.back()">Atrás</a>
    </div>
</div>
<% include partials/_footer %>