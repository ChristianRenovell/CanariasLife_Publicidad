<% include partials/_header %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/update/<%= data.id %>/<%= value %>/<%= value2 %>" method="POST">
          <div class="form-group">
            <input type="text" name="name" value="<%= data.name%>" placeholder="Nombre" class="form-control" autofocus>
          </div>
          <div class="form-group">
            <input type="text" name="banner" value="<%= data.banner%>" placeholder="url Banner" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="video" value="<%= data.video%>" placeholder="url Video" class="form-control">
          </div>
          <button type="submit" class="btn btn-primary">
            Actualizar
          </button>
        </form>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer %>
