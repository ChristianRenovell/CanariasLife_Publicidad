<% include partials/_header %>
<div class="container mt-5">  
    <div class="row text-center">
        <div class="col-lg-6">
            <h1 class="text-uppercase mb-4"><%= nameBanner %></h1>
            <h2 class="text-primary">Historial del Banner</h2>
            <a href="/publi_services/reportpdfBanner/<%= id %>/<%= nameBanner %>" class="btn btn-outline-danger mt-3 mb-3 btn-block">Generar PDF</a>
            <% if (dataBanner) { %>
            <% for(var i = 0; i < dataBanner.length; i++) { %>
            <tr>
                <td><%= dataBanner[i].date.getDate() %>-<%= dataBanner[i].date.getMonth() %>-<%= dataBanner[i].date.getFullYear() %> <%= dataBanner[i].date.getHours() %>:<%= dataBanner[i].date.getMinutes() %>:<%= dataBanner[i].date.getSeconds() %> Hora de las Islas Canarias.</td>
                <td>
                </td>
            </tr>
            <% } %>
            <% } %>
        </div>
        <div class="col-lg-6">
            <h1 class="text-uppercase mb-4"><%= nameVideo %></h1>
            <h2 class="text-primary">Historial del Video</h2>
            <a href="/publi_services/reportpdfVideo/<%= id %>/<%= nameVideo %>" class="btn btn-outline-danger mt-3 mb-3 btn-block" id="testButton">Generar PDF</a>
            <% if (dataVideo) { %>
            <% for(var i = 0; i < dataVideo.length; i++) { %>
            <tr>
                <td><%= dataVideo[i].date.getDate() %>-<%= dataVideo[i].date.getMonth() %>-<%= dataVideo[i].date.getFullYear() %> <%= dataVideo[i].date.getHours() %>:<%= dataVideo[i].date.getMinutes() %>:<%= dataVideo[i].date.getSeconds() %> Hora de las Islas Canarias.</td>
            </tr>
            <% } %>
            <% } %>
         
        </div>
    </div>
    <div class="text-center">
        <a class="btn btn-primary mt-5" style="width: 20%;" href="javascript:history.back()">Atr√°s</a>
    </div>
</div>
<% include partials/_footer %>