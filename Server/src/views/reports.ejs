<% include partials/_header %>
    <div class="container mt-5">
        <div class="row">
            <div class="col-lg-12">
                <% if (data) { %>
                <% for(var i = 0; i < data.length; i++) { %>
                <tr>
                    <td><%= data[i].id %></td>
                    <td><%= data[i].date %></td>
                    <td>
                        <a href="/report/<%= data[i].id %>" class="btn btn-info">Generar PDF</a>
                    </td>
                </tr>
                <% } %>
                <% } %>
            </div>
        </div>
</div>



<% include partials/_footer %>